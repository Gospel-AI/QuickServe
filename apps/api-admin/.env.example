# Server
PORT=3001
NODE_ENV=development

# Database (同じDBを共有)
DATABASE_URL="postgresql://postgres:password@localhost:5432/quickserve?schema=public"

# JWT (api-publicとは異なるシークレットを使用)
JWT_SECRET="admin-super-secret-jwt-key-change-in-production"
JWT_EXPIRES_IN="1d"

# Admin許可IPリスト (本番環境用)
ALLOWED_IPS=""

# Admin許可ドメイン
ADMIN_DOMAIN="admin.quickserve.com"
